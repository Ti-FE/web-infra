{
  "name": "@ti-fe/cli",
  "version": "0.11.0",
  "description": "A helper tool for better web infra integration",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "nodemon --exec \"yarn build\" -e ts",
    "prebuild": "rimraf dist",
    "prepublish": "npm run build"
  },
  "bin": {
    "wi": "bin/cli.js"
  },
  "engines": {
    "node": ">= 12",
    "npm": ">= 7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ti-FE/web-infra.git",
    "directory": "packages/cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "dist"
  ],
  "dependencies": {
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@iarna/toml": "^2.2.5",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/prompts": "^2.0.12",
    "@types/semver": "^7.3.6",
    "cac": "^6.7.3",
    "chalk": "^4.1.1",
    "cosmiconfig": "^7.0.0",
    "detect-indent": "^6.0.0",
    "json5": "^2.2.0",
    "log-symbols": "^5.0.0",
    "nodemon": "^2.0.7",
    "prompts": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.48.0",
    "shq": "^1.0.2",
    "tslib": "^2.2.0"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "node_modules/*"
    ],
    "delay": "1000",
    "watch": [
      "src"
    ]
  }
}
