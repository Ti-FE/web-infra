{
  "name": "@ti-fe/cli",
  "version": "0.11.1",
  "description": "A helper tool for better web infra integration",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "targets": {
    "main": {
      "includeNodeModules": {
        "shelljs": false
      }
    }
  },
  "scripts": {
    "build": "parcel build --no-scope-hoist",
    "watch": "parcel watch",
    "prebuild": "rimraf dist",
    "prepublish": "npm run build"
  },
  "bin": {
    "wi": "bin/cli.js"
  },
  "engines": {
    "node": ">= 12",
    "npm": ">= 7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ti-FE/web-infra.git",
    "directory": "packages/cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "dist"
  ],
  "dependencies": {
    "@rtk-incubator/rtk-query-codegen-openapi": "^0.5.0",
    "openapi-types": "^9.1.0",
    "shelljs": "^0.8.4",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@iarna/toml": "^2.2.5",
    "@types/node": "^16.4.7",
    "@types/prompts": "^2.0.12",
    "@types/semver": "^7.3.6",
    "@types/shelljs": "^0.8.9",
    "cac": "^6.7.11",
    "chalk": "^4.1.1",
    "cosmiconfig": "^7.0.0",
    "detect-indent": "^7.0.0",
    "json5": "^2.2.0",
    "log-symbols": "^5.0.0",
    "parcel": "^2.0.0",
    "prompts": "^2.4.1",
    "rimraf": "^3.0.2",
    "semver": "^7.3.5",
    "typescript": "^4.3.5"
  }
}
