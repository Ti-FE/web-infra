{
  "name": "@pingcap/webinfra-cli",
  "description": "A helper tool for better web infra integration",
  "license": "MIT",
  "main": "dist/cli.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "nodemon --exec \"yarn build\" -e ts",
    "prebuild": "rimraf dist"
  },
  "bin": {
    "wi": "bin/cli.js"
  },
  "version": "0.1.0",
  "dependencies": {},
  "engines": {
    "node": ">= 12"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "cac": "^6.7.3",
    "chalk": "^4.1.1",
    "log-symbols": "^5.0.0",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "rollup": "^2.48.0",
    "shq": "^1.0.2",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "node_modules/*"
    ],
    "delay": "1000",
    "watch": [
      "src"
    ]
  }
}
