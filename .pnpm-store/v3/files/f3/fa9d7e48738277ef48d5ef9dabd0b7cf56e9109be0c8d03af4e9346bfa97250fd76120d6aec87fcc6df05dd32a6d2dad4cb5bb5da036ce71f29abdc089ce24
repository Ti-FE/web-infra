import { Plugin } from 'vite';

interface Options {
    /**
     * @default 'package.json'
     */
    packageJsonPath?: string;
    /**
     * Field name in package.json to merge with Vite's config
     *
     * @default 'vite'
     */
    field?: string;
}
declare function VitePluginPackageConfig(options?: Options): Plugin;

export { Options, VitePluginPackageConfig as default };
